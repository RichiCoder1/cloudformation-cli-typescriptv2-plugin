{
  "name": "test-type",
  "version": "1.0.0",
  "description": "AWS custom resource provider named Test::Test::Test.",
  "private": true,
  "main": "dist/handlers.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "type-check": "npx tsc",
    "generate": "cfn-js generate --schema test-test-test.json",
    "prebuild": "tsup-node",
    "invoke:test": "sam local invoke -t build/template.yaml TestEntrypoint",
    "build": "sam build --build-dir build --use-container",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "optionalDependencies": {
    "aws-sdk": "^2.656.0"
  },
  "dependencies": {
    "@amazon-web-services-cloudformation/cloudformation-cli-typescriptv2-lib": "^2.0.0-a0"
  }
}
