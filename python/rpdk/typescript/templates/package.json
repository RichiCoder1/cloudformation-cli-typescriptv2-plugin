{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "private": true,
  "main": "dist/handlers.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "type-check": "npx tsc",
    "generate": "cfn-js generate --schema {{ schema }}",
    "prebuild": "tsup",
    "invoke:test": "sam local invoke -t build/template.yaml TestEntrypoint",
    "build": "sam build{% if use_docker %} --use-container{% endif %} --build-dir build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "{{lib_name}}": "{{lib_path}}"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "optionalDependencies": {
    "aws-sdk": "^2.656.0"
  }
}
