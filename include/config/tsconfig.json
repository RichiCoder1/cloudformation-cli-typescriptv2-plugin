{
  "compilerOptions": {
    "target": "ES2022",
    "module": "Node16",
    "moduleResolution": "node16",
    "noImplicitAny": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "noEmit": true,
    "rootDirs": [
      "src",
      ".cfn/typescript/generated"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "$cfn/*": [
        ".cfn/typescript/generated/*"
      ]
    }
  }
}
